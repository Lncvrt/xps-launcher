{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "process:default",
    "fs:default",
    "fs:allow-appconfig-write",
    "fs:allow-appconfig-read",
    "fs:allow-exists",
    "fs:allow-create",
    "fs:allow-mkdir",
    "fs:allow-read-file",
    "fs:allow-remove"
  ]
}